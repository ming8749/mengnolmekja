<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org/DTD config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>
	<settings>
		<setting name="mapUnderscoreToCamelCase" value="true"/> <!-- setter 이름 언더바 차이면 무시하고 넣음 -->
		
	</settings>
	
	<typeAliases>
		<typeAlias type="com.kh.dog.main.model.vo.Reservation" alias="Reservation"/>
		<typeAlias type="com.kh.dog.member.model.vo.Member" alias="Member"/>	
		<typeAlias type="com.kh.dog.member.model.vo.Dog" alias="Dog"/>	
		<typeAlias type="com.kh.dog.main.model.vo.Board" alias="Board"/>
		<typeAlias type="com.kh.dog.main.model.vo.Picture" alias="Picture"/>
		<typeAlias type="com.kh.dog.main.model.vo.Request" alias="Request"/>
		<typeAlias type="com.kh.dog.main.model.vo.Board" alias="Board"/>
		<typeAlias type="com.kh.dog.main.model.vo.Reply" alias="Reply"/>
		<typeAlias type="com.kh.dog.main.model.vo.Notice" alias="Notice"/>
	</typeAliases>

	<mappers>
		<mapper resource="mappers/member-mapper.xml"/>
		<mapper resource="mappers/reservation-mapper.xml"/>
		<mapper resource="mappers/manager-mapper.xml"/>
		<mapper resource="mappers/galary-mapper.xml"/>
		<mapper resource="mappers/request-mapper.xml"/>
		<mapper resource="mappers/notice-mapper.xml"/>
		<mapper resource="mappers/board-mapper.xml"/>
	</mappers>
</configuration>
